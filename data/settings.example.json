{
    "settings": {
        "daysToKeep": 2,
        "sendToTrash": true,
        "maxSizeInMb": 500
    },
    "orderedFiles": [],
    "paths": {
        "sourcePath": "C:\\User\\Windows\\Downloads",
        "destinationPath": "C:\\User\\Windows\\Downloads\\Organized"
    },
    "sortingRules": [
        {
            "folderName": "Gemini",
            "matchBy": "regex",
            "patterns": [
                ".*Gemini.*"
            ]
        },
        {
            "folderName": "Word",
            "matchBy": "extension",
            "patterns": [
                ".doc",
                ".docx",
                ".dot"
            ]
        },
        {
            "folderName": "PowerPoint",
            "matchBy": "extension",
            "patterns": [
                ".ppt",
                ".pptx",
                ".pps",
                ".pot"
            ]
        },
        {
            "folderName": "Excel",
            "matchBy": "extension",
            "patterns": [
                ".xls",
                ".xlsx",
                ".xlsm",
                ".xlt"
            ]
        },
        {
            "folderName": "Video",
            "matchBy": "extension",
            "patterns": [
                ".avi",
                ".divx",
                ".mov",
                ".mp4",
                ".mpg",
                ".mkv",
                ".wmv",
                ".wpl"
            ]
        },
        {
            "folderName": "Image",
            "matchBy": "extension",
            "patterns": [
                ".JPEG",
                ".JPG",
                ".jpg",
                ".jpeg",
                ".png",
                ".bmp",
                ".ico",
                ".svg",
                ".webp",
                ".gif",
                ".psd",
                ".heic",
                ".nef",
                ".crw",
                ".ai",
                ".id"
            ]
        },
        {
            "folderName": "Audio",
            "matchBy": "extension",
            "patterns": [
                ".mp3",
                ".mid",
                ".wav",
                ".wma",
                ".cda",
                ".ogm",
                ".acc",
                ".ac3",
                ".flac",
                ".aym"
            ]
        },
        {
            "folderName": "Compressed",
            "matchBy": "extension",
            "patterns": [
                ".zip",
                ".rar",
                ".rar5",
                ".7z",
                ".ace",
                ".gz",
                ".tar.bz2"
            ]
        },
        {
            "folderName": "PDF",
            "matchBy": "extension",
            "patterns": [
                ".pdf"
            ]
        },
        {
            "folderName": "Gemini",
            "matchBy": "extension",
            "patterns": []
        },
        {
            "folderName": "Other",
            "matchBy": "extension",
            "patterns": []
        }
    ],
    "defaultFolder": "Other"
}