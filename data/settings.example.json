{
    "paths": {
        "sourcePath": "C:\\Users\\LMayNasa\\Downloads",
        "destinationPath": "C:\\Users\\LMayNasa\\Downloads\\Organized"
    },
    "settings": {
        "maxSizeInMb": 5000
    },
    "defaultLifecycle": {
        "enabled": true,
        "action": "trash",
        "daysToKeep": 2
    },
    "sortingRules": [
        {
            "folderName": "TV\\Series",
            "matchBy": "regex",
            "patterns": [
                ".*S\\d{2}E\\d{2}.*"
            ],
            "lifecycle": {
                "enabled": false,
                "action": "delete",
                "daysToKeep": 30
            }
        },
        {
            "folderName": "TV\\Movies",
            "matchBy": "regex",
            "patterns": [
                ".*(1080p|...).*\\d{4}.*",
                ".*\\d{4}.*(1080p|...).*"
            ],
            "lifecycle": {
                "enabled": false,
                "action": "delete",
                "daysToKeep": 30
            }
        },
        {
            "folderName": "Word",
            "matchBy": "extension",
            "patterns": [
                ".doc",
                ".docx",
                ".dot"
            ],
            "lifecycle": null
        },
        {
            "folderName": "PowerPoint",
            "matchBy": "extension",
            "patterns": [
                ".ppt",
                ".pptx",
                ".pps",
                ".pot"
            ],
            "lifecycle": null
        },
        {
            "folderName": "Excel",
            "matchBy": "extension",
            "patterns": [
                ".xls",
                ".xlsx",
                ".xlsm",
                ".xlt"
            ],
            "lifecycle": null
        },
        {
            "folderName": "Video",
            "matchBy": "extension",
            "patterns": [
                ".avi",
                ".divx",
                ".mov",
                ".mp4",
                ".mpg",
                ".mkv",
                ".wmv",
                ".wpl"
            ],
            "lifecycle": null
        },
        {
            "folderName": "Image",
            "matchBy": "extension",
            "patterns": [
                ".JPEG",
                ".JPG",
                ".jpg",
                ".jpeg",
                ".png",
                ".bmp",
                ".ico",
                ".svg",
                ".webp",
                ".gif",
                ".psd",
                ".heic",
                ".nef",
                ".crw",
                ".ai",
                ".id"
            ],
            "lifecycle": null
        },
        {
            "folderName": "Audio",
            "matchBy": "extension",
            "patterns": [
                ".mp3",
                ".mid",
                ".wav",
                ".wma",
                ".cda",
                ".ogm",
                ".acc",
                ".ac3",
                ".flac",
                ".aym"
            ],
            "lifecycle": null
        },
        {
            "folderName": "Compressed",
            "matchBy": "extension",
            "patterns": [
                ".zip",
                ".rar",
                ".rar5",
                ".7z",
                ".ace",
                ".gz",
                ".tar.bz2"
            ],
            "lifecycle": null
        },
        {
            "folderName": "PDF",
            "matchBy": "extension",
            "patterns": [
                ".pdf"
            ],
            "lifecycle": null
        }
    ],
    "defaultFolder": "Other",
    "folderRules": [
        {
            "ruleName": "MoveSeries",
            "matchBy": "regex",
            "patterns": [
                ".*S\\d{2}E\\d{2}.*"
            ],
            "action": "process_contents",
            "deleteEmptyAfterProcessing": true,
            "destinationFolder": "TV\\Series",
            "lifecycle": {
                "enabled": false,
                "action": "trash",
                "daysToKeep": 30
            }
        },
        {
            "ruleName": "MoveMovies",
            "matchBy": "regex",
            "patterns": [
                ".*(1080p|...).*\\d{4}.*",
                ".*\\d{4}.*(1080p|...).*"
            ],
            "action": "process_contents",
            "deleteEmptyAfterProcessing": true,
            "destinationFolder": "TV\\Movies",
            "lifecycle": {
                "enabled": false,
                "action": "trash",
                "daysToKeep": 30
            }
        }
    ],
    "defaultFolderAction": "ignore",
    "orderedFiles": []
}